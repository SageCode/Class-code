import numpy as np
g = 9.8
h_0 = 10
y = np.arange(10,0.5,-0.5)
y_2 = np.arange(9.5,0,-0.5)
t = (2 * (h_0 - y) / g )**0.5
t_2 = (2 * (h_0 - y_2) / g )**0.5
atime = t_2 - t
ay = y_2 - y

av = ay/atime
print("The average velocity for the ball found in the last example is:")
print(str(av) + str("m/s"))
